{
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "prettier": "^3.4.2",
    "turbo": "^2.3.3",
    "lefthook": "^1.10.2"
  },
  "packageManager": "bun@1.1.42",
  "scripts": {
    "dev": "turbo dev",
    "db": "docker compose up -d",
    "lint": "biome check . --fix",
    "generate": "bunx prisma generate --schema ./apps/api/prisma/schema.prisma",
    "setup": "bunx lefthook install && bun db && bun generate"
  },
  "workspaces": ["apps/*"],
  "dependencies": {
    "elysia": "^1.2.10"
  },
  "patchedDependencies": {
    "@grotto/logysia@0.1.6": "patches/@grotto%2Flogysia@0.1.6.patch"
  }
}
